<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.tecsup.com.appcooperativa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AccountActivity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".AccountActivity.SignupActivity" />
        <activity android:name=".AccountActivity.ResetPasswordActivity" />
        <activity android:name=".DashboardActivity.ActivityDashboard" />
        <activity android:name=".DashboardActivity.Activity_coop"
            android:theme="@style/ThemeCoop" />

        <activity android:name=".DashboardActivity.Activity_servicios"
            android:theme="@style/ThemeServicios" />

        <activity android:name=".DashboardActivity.Activity_socios"
            android:theme="@style/ThemeSocios"/>

        <activity android:name=".DashboardActivity.Activity_estadisticas"
            android:theme="@style/ThemeEstadisticas" />
        <activity android:name=".DashboardActivity.Activity_tablas"
            android:theme="@style/ThemeTablas" />
        <activity android:name=".DashboardActivity.Activity_mapa"
            android:theme="@style/ThemeMapa" />
        <activity android:name=".DashboardActivity.Activity_productos"
            android:theme="@style/ThemeProductos" />
        <activity android:name=".DashboardActivity.Activity_bazar"
            android:theme="@style/ThemeBazar" ></activity>
    </application>

</manifest>